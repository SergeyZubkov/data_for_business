.bg-yellow-image
	display block
	position absolute
	top 0
	left 0
	right 0
	height 688px
	background #FFE694 url('../title-bg.png')
	background-position 95% 100%
	background-size cover
	background-repeat no-repeat
	z-index -1
	
@media screen and (max-width: 480px)
	.bg-yellow-image
		background-size 350% 